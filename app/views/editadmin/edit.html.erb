<%=form_for @user,:url=>{:action=>"update"} do |e| %>
FullName
<%=e.text_field :fullname%><%=@error[:fullname] if @error && @error[:fullname]%><br>
Email
<%=e.email_field :email%><%=@error[:email] if @error && @error[:email]%><br>
Phone
<%=e.text_field :phone%><br>
Mobile
<%=e.text_field :mobile%><br>
<%=e.submit "Modify"%>
<%end%>
